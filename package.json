{
  "name": "fixmyevent",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:strict": "next lint --strict",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit --strict",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "db:status": "tsx -e \"import { checkDatabaseStatus } from './src/lib/seed'; checkDatabaseStatus();\"",
    "db:clear": "tsx -e \"import { clearDatabase } from './src/lib/seed'; clearDatabase();\"",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "cost:setup": "chmod +x scripts/setup-cost-optimization.sh && ./scripts/setup-cost-optimization.sh",
    "cost:monitor": "chmod +x scripts/monitor-costs.sh && ./scripts/monitor-costs.sh",
    "cost:optimize": "tsx scripts/cost-optimization.ts",
    "seed": "tsx src/lib/seed.ts",
    "analyze": "ANALYZE=true next build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
